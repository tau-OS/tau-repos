[tauOS]
name=tauOS $taurelease - $basearch
baseurl=https://repos.fyralabs.com/tau$taurelease
enabled=1
countme=1
metadata_expire=7d
# We need to fix this, but rpm-ostree with gnome software is borked with this
repo_gpgcheck=0
type=rpm
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-tauOS-$taurelease-$basearch